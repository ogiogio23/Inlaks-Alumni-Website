        <script>
$(function(){
    $('#alumniId').on('input', function () {
        $('body').addClass("loading");
     var alumniId = $("#alumniId").val();
    $.ajax({
      type: "GET",
	  crossDomain: true,
	  url: "https://run.mocky.io/v3/33d42a7b-0ab4-4309-951b-8354638f5c33?alumniId" + alumniId,

      cache: false,
      success: function (data) {
          $("body").removeClass("loading");

          if(data == null || data == undefined ){
              $("#error").html("<p style='color:#ff1a1a;'>Remote Server Error.</p>");
              return;
          }

	  if(data.ResponseCode != "00"){
	      $("#error").html("<p style='color:#ff1a1a;'>Invalid Alumni </p>");
	  return;
	  }

	  $("#hiddenField").val(data.ResponseCode);

      },
      error: function (error) {
          $("body").removeClass("loading");
          $("#error").html("<p style='color:#ff1a1a;'>Network Error Occurred. </p>");
      }
    });
    });

 $('#alumniId').on('mouseout', function () {
     $("#error").html("");
 });
});
        </script>

